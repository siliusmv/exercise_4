{"chunk_definitions":[{"chunk_id":"csetup_chunk","chunk_label":"setup","document_id":"7C0DC1AC","expansion_state":0,"options":{"cache":false,"engine":"r","include":false,"label":"setup"},"row":19,"row_count":1,"visible":true},{"chunk_id":"cvkokye79ll9d","chunk_label":"unnamed-chunk-1","document_id":"7C0DC1AC","expansion_state":0,"options":{"engine":"r","label":"unnamed-chunk-4"},"row":40,"row_count":1,"visible":true},{"chunk_id":"cvp6vfi516sp4","chunk_label":"unnamed-chunk-2","document_id":"7C0DC1AC","expansion_state":0,"options":{"engine":"r","label":"unnamed-chunk-11"},"row":57,"row_count":1,"visible":true},{"chunk_id":"cnxszdxg70gt0","chunk_label":"unnamed-chunk-3","document_id":"7C0DC1AC","expansion_state":0,"options":{"engine":"python","label":"unnamed-chunk-66"},"row":71,"row_count":1,"visible":true}],"default_chunk_options":{"error":false},"doc_write_time":1522532472,"external_chunks":{"unnamed-chunk-5":["# Euclidian distance function","eucDist <- function(all_points, point){","  ","  res <- sweep(all_points, 2, point, \"-\")","  res <- res^2","  res <- apply(res, 1, sum)","  res <- sqrt(res)","  ","  return(res)","}","","","# Find k nearest neighbours","findKnn <- function(all_points, point, distFunc, k, values){","  ","  distances <- distFunc(all_points, point)","  ","  ord <- order(distances)","  ","  res <- list(distances = distances[1:k],","              values = values[ord][1:k])","  ","  return(res)","  ","}","","","knnReg <- function(k, data, point){","  ","  all_points <- as.matrix(data[, -dim(data)[2]])","  values <- data[, dim(data)[2]]","  ","  closest <- findKnn(all_points = all_points,","                     point = point,","                     k = k,","                     values = values,","                     distFunc = eucDist)","  ","  return(mean(closest$values))","}"]},"working_dir":null}